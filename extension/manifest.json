{
  "manifest_version": 3,
  "name": "Req Hog",
  "version": "1.0",
  "permissions": [
    "storage",
    "webNavigation",
    "debugger",
    "activeTab"
  ],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}
